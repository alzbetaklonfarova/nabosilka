<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.home.HomeFragment"
    android:background="@color/sandy">


    <TextView
        android:id="@+id/Nasobky"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Násobky"
        android:textAlignment="center"
        android:textSize="60sp"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/TypHry"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Typ hry"
        android:textAlignment="center"
        android:textSize="60sp"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/Děleni"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dělení"
        android:textAlignment="center"
        android:textSize="60sp"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/Děleni"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Peti" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TypHry" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/Peti"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Nasobky" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/TypHry"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Děleni" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <Button
        android:id="@+id/Vyber"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="Vybirej"
        android:text="Výběr z možností"
        android:theme="@style/MojeTlacitko"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView5"
        app:layout_constraintEnd_toStartOf="@+id/Psani"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/Psani"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:theme="@style/MojeTlacitko"
        android:onClick="PsatVysledek"
        android:text="Psaní výsledku"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/Vyber" />

    <CheckBox
        android:id="@+id/Ano"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ano"
        android:theme="@style/MyCheckBox"
        android:textColor="@color/black"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView7"
        app:layout_constraintEnd_toStartOf="@+id/Ne"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent" />

    <CheckBox
        android:id="@+id/Ne"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ne"
        android:theme="@style/MyCheckBox"
        android:textColor="@color/black"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/Ano" />

    <CheckBox
        android:id="@+id/Nuly"
        android:layout_width="101dp"
        android:layout_height="wrap_content"
        android:text="Nuly"
        android:theme="@style/MyCheckBox"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/Jedne"
        app:layout_constraintEnd_toStartOf="@+id/Jedne"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent" />

    <CheckBox
        android:id="@+id/Jedne"
        android:layout_width="102dp"
        android:layout_height="wrap_content"
        android:text="Jedné"
        android:theme="@style/MyCheckBox"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView6"
        app:layout_constraintEnd_toStartOf="@+id/Dvou"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/Nuly" />

    <CheckBox
        android:id="@+id/Dvou"
        android:layout_width="101dp"
        android:layout_height="wrap_content"
        android:text="Dvou"
        android:theme="@style/MyCheckBox"
        android:textSize="20sp"
        android:textColor="@color/black"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView6"
        app:layout_constraintEnd_toStartOf="@+id/Tri"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/Jedne" />

    <CheckBox
        android:id="@+id/Tri"
        android:layout_width="102dp"
        android:layout_height="wrap_content"
        android:text="Tří"
        android:theme="@style/MyCheckBox"
        android:textSize="20sp"
        android:textColor="@color/black"
        app:layout_constraintBaseline_toBaselineOf="@+id/Dvou"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/Dvou" />

    <CheckBox
        android:id="@+id/Ctyr"
        android:layout_width="101dp"
        android:layout_height="wrap_content"
        android:text="Čtyř"
        android:theme="@style/MyCheckBox"
        android:textSize="20sp"
        android:textColor="@color/black"
        app:layout_constraintBaseline_toBaselineOf="@+id/Peti"
        app:layout_constraintEnd_toStartOf="@+id/Peti"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent" />

    <CheckBox
        android:id="@+id/Peti"
        android:layout_width="102dp"
        android:layout_height="wrap_content"
        android:text="Pěti"
        android:theme="@style/MyCheckBox"
        android:textSize="20sp"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintEnd_toStartOf="@+id/Sesti"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/Ctyr"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <CheckBox
        android:id="@+id/Sesti"
        android:layout_width="101dp"
        android:layout_height="wrap_content"
        android:theme="@style/MyCheckBox"
        android:text="Šesti"
        android:textSize="20sp"
        android:textColor="@color/black"
        app:layout_constraintBaseline_toBaselineOf="@+id/Peti"
        app:layout_constraintEnd_toStartOf="@+id/Sedmi"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/Peti" />

    <CheckBox
        android:id="@+id/Sedmi"
        android:layout_width="102dp"
        android:layout_height="wrap_content"
        android:text="Sedmi"
        android:theme="@style/MyCheckBox"
        android:textSize="20sp"
        android:textColor="@color/black"
        app:layout_constraintBaseline_toBaselineOf="@+id/Sesti"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/Sesti" />

    <CheckBox
        android:id="@+id/Osmi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Osmi"
        android:theme="@style/MyCheckBox"
        android:textSize="20sp"
        android:textColor="@color/black"
        app:layout_constraintBaseline_toBaselineOf="@+id/Deviti"
        app:layout_constraintEnd_toStartOf="@+id/Deviti"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent" />

    <CheckBox
        android:id="@+id/Deviti"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Devíti"
        android:theme="@style/MyCheckBox"
        android:textSize="20sp"
        android:textColor="@color/black"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView4"
        app:layout_constraintEnd_toStartOf="@+id/Desiti"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/Osmi" />

    <CheckBox
        android:id="@+id/Desiti"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Desíti"
        android:theme="@style/MyCheckBox"
        android:textSize="20sp"
        android:textColor="@color/black"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/Deviti" />

</androidx.constraintlayout.widget.ConstraintLayout>